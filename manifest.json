{
    "manifest_version": 2,
    "content_scripts": [ {
        "exclude_globs":    [  ],
        "include_globs":    [ "*" ],
        "js":               [ "HUGELOL_Tools.user.js" ],
        "matches":          [   "https://hugelol.com/*",
                                "http://hugelol.com/*",
                                "https://www.hugelol.com/*",
                                "http://www.hugelol.com/*"
                            ],
        "permissions": [ "webRequest" ],
        "run_at": "document_end"
    } ],
    "converted_from_user_script": true,
    "description":  "HUGELOL extra functionality",
    "name":         "HUGELOL Tools",
    "version":      "1.1"
}
